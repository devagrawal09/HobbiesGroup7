@{
    ViewData["Title"] = "Database";
}
<div class="text-center;">

    <table class="table table-bordered table-striped">
        <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Bio</td>
            <td>Hobby</td>
            <td>HobbyDescription</td>
        </tr>
        <tr>
            @{
                foreach (var Data in Model)
                {
                    <td>@Data.TeamMemberId</td>
                    <td>@Data.Name</td>
                    <td>@Data.Bio</td>
                    <td>@Data.HobbyTitle</td>
                    <td>@Data.HobbyDescription</td>
                }
            }
        </tr>
        </table>
    </div>