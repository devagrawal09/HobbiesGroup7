@model IEnumerable<TeamMember>
@{
    ViewData["Title"] = "Home Page";
}
<body>
    <img src="~/images/Vector1.png" class="bodyBackground1">
    <img src="~/images/Vector2.png" class="bodyBackground2">
    <img src="~/images/Vector3.png" class="bodyBackground3">
    <div class="text-center">
        <h3>Whose hobbies would you like to find out about?</h3>
        <div>
            <ul class="homeName">
                @foreach (var member in Model)
                {
                    <a asp-area=""
                       asp-controller="Team"
                       asp-action="@member.TeamMemberId"
                    >
                        <li class="navItem">
                            <img src="@member.Avatar" class="user" />
                            <br>
                            @member.Name
                        </li>
                    </a>
                }
            </ul>
        </div>
    </div>
</body>
